{% extends "base.twig" %}

{% block content %}
<article class="">
	<div class="container">
		<div class="row section__header">
			<div class="col-12">
				<h2>{{ foo }}</h2>
				<p>{{ qux }}</p>
				{% for post in posts %}
				{% include ['tease-'~post.post_type~'.twig', 'tease.twig'] %}
				{% endfor %}

				{% include 'components/pagination.twig' with { pagination: posts.pagination({show_all: false, mid_size: 3, end_size: 2}) } %}
			</div>
		</div>
	</div>
</article>
{% endblock %}
