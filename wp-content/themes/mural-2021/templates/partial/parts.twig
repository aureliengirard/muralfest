{% for item in content_items -%}

    {%- for inner_item in item.contenu -%}
    {%- set bg_img = TimberImage(inner_item.bg_img).src -%}
    {%- set hide = item.hide -%}

    <section id="{{item.nom_de_la_section|replace({' ': '-'})}}" class="{{inner_item.acf_fc_layout}}" {%- if hide or bg_img != null %}style="{%- endif %}{%- if hide %}display:none;{%- endif %}{% if bg_img != null %}background-image:url('{{bg_img}}'); {% endif %}{%- if hide or bg_img != null %}"{%- endif %}>
        {%- if inner_item.acf_fc_layout == 'application' -%}
            {% include "partial/part-application.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'texte_bouton' -%}
            {%- include "partial/part-texte_bouton.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'video' -%}
            {%- include "partial/part-video.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'appels_a_laction' -%}
            {%- include "partial/part-appels_a_laction.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'titre_section' -%}
            {%- include "partial/part-titre_section.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'deux_colonnes' -%}
            {%- include "partial/part-deux_colonnes.twig" with { layout: 'deux_colonnes' } -%}
        {%- elseif inner_item.acf_fc_layout == 'trois_colonnes' -%}
            {%- include "partial/part-deux_colonnes.twig" with { layout: 'trois_colonnes' } -%}
        {%- elseif inner_item.acf_fc_layout == 'formulaire_infolettre' -%}
            {%- include "partial/part-formulaire_infolettre.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'articles_du_blog' -%}
            {%- include "partial/part-articles_du_blog.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'texte_pleine_largeur' -%}
            {%- include "partial/part-texte_pleine_largeur.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'galerie' -%}
            {%- include "partial/part-galerie.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'partenaires' -%}
            {%- include "partial/part-partenaires.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'evenements_a_venir' -%}
            {%- include "partial/part-evenements_a_venir.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'image_pleine_largeur' -%}
            {%- include "partial/part-image_pleine_largeur.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'image_et_texte' -%}
            {%- include "partial/part-image_et_texte.twig" -%}
        {%- elseif inner_item.acf_fc_layout == 'faq' -%}
            {%- include "partial/part-faq.twig" -%}
        {%- endif -%}
    </section>
    {# Nom:{{item.nom_de_la_section}} - Layout name: {{inner_item.acf_fc_layout}} Loop:{{loop.index}} #}
    {%- endfor -%}
{%- endfor %}